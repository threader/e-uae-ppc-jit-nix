DIVUREG2REGW 0   1000 ddd0 11SS Ssss   0 0     regD    regD
DIVUMEM2REGW 0   1000 ddd0 11SS Ssss   0 0     indA,indApW,indmAW,indAd16,indAcp,indPCcp,indPCd16,absW,absL    regD

;*** DIVU.L/DIVS.L
DIVIMM2REGL  0   0100 1100 0111 1100   0 0     none    none
DIVREG2REGL  0   0100 1100 01SS Ssss   0 0     regD    none
DIVMEM2REGL  0   0100 1100 01SS Ssss   0 0     indA,indApL,indmAL,indAd16,indAcp,indPCcp,indPCd16,absW,absL    none

;*** NEG
NEGREGL      0   0100 0100 10DD Dddd   0 0     none    regD
NEGREGW      0   0100 0100 01DD Dddd   0 0     none    regD
NEGREGB      0   0100 0100 00DD Dddd   0 0     none    regD

