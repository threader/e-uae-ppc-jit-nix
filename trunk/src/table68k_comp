;***
;*** E-UAE JIT compiling
;***
;*** Opcode table
;***
;*** Lists all JIT supported instuctions with the required flags.
;*** If an instruction was not listed here then it is not supported
;*** by the JIT compiling and the execution falls back to the
;*** interpretive emulation.

;*** Format:
;*** opcode_name implemented binary_code_in_4_parts jump constjump srcaddr destaddr
;***   implemented - this instruction was implemented or not (1/0)
;***   binary code - contains "S" and "s" as source addr/reg, 
;***                          "D" and "d" as destination bits addr/reg
;***                          "x" for ignoring that bit
;***   jump - compile stopping opcode (branch mostly) (1/0)
;***   constjump - constant jump opcode (1/0)
;***   srcaddr - source addressing
;***   destaddr - destination addressing
;***
;*** Addressing markups:
;***
;*** regD    - Dx register: Dx
;*** regA    - Ax register: Ax
;*** indA    - Ax indirect: (Ax)
;*** indmAy  - Ax indirect with predecrement : -(Ax), y - size: B, W, L
;*** indApy  - Ax indirect with postincrement: (Ax)+, y - size: B, W, L
;*** immed   - immediate value: #x - size: B, W, L
;*** immedQ  - immediate value in opcode
;*** indAd16 - Ax indirect d16 indexed: d16(Ax)
;*** indPCd16 - PC indirect d16 indexed: d16(PC)
;*** absW    - absolute word sized address: x.w
;*** absL    - absolute longword sized address: x.l
;*** indAcp  - Ax indirect with complex displacement, eg: ([bd, An] Rn, od)
;*** indPCcp - PC indirect with complex displacement, eg: ([bd, PC] Rn, od)

#Addressing
;address   modus  register
;none should be the first int the list
none        xxx    xxx
regD        000    rrr
regA        001    rrr
indA        010    rrr
indmAL      100    rrr
indmAW      100    rrr
indmAB      100    rrr
indApL      011    rrr
indApW      011    rrr
indApB      011    rrr
immedL      111    100
immedW      111    100
immedB      111    100
immedQ      rrr    rrr
indAd16     101    rrr
indPCd16    111    010
absW        111    000
absL        111    001
indAcp      110    rrr
indPCcp     111    011

;Conditional code bits
CC_cc   0    100
CC_cs   0    101
CC_eq   0    111
CC_ge   1    100
CC_gt   1    110
CC_hi   0    010
CC_le   1    111
CC_ls   0    011
CC_lt   1    101
CC_mi   1    011
CC_ne   0    110
CC_pl   1    010
CC_vc   1    000
CC_vs   1    001
CC_t    0    000
CC_f    0    001

;FPU Conditional code bits
FCC_f      000    000
FCC_eq     000    001
FCC_ogt    000    010
FCC_oge    000    011
FCC_olt    000    100
FCC_ole    000    101
FCC_ogl    000    110
FCC_or     000    111
FCC_un     001    000
FCC_ueq    001    001
FCC_ugt    001    010
FCC_uge    001    011
FCC_ult    001    100
FCC_ule    001    101
FCC_ne     001    110
FCC_t      001    111
FCC_sf     010    000
FCC_seq    010    001
FCC_gt     010    010
FCC_ge     010    011
FCC_lt     010    100
FCC_le     010    101
FCC_gl     010    110
FCC_gle    010    111
FCC_ngle   011    000
FCC_ngl    011    001
FCC_nle    011    010
FCC_nlt    011    011
FCC_nge    011    100
FCC_ngt    011    101
FCC_sne    011    110
FCC_st     011    111

#Opcodes
;Format:
;name      impl  ext siz  bincode            jump|cjmp  srcaddr destaddr
;---------------------------------------------------------------------
;*** Data moving instructions
;*** Move register-register
MOVREG2REG   1    0   4   0010 dddD DDSS Ssss   0 0     regD,regA    regD
MOVREG2REG   1    0   2   0011 dddD DDSS Ssss   0 0     regD,regA    regD
MOVREG2REG   1    0   1   0001 dddD DDSS Ssss   0 0     regD     regD

;If the target is an address register, then flags kept unchanged
MOVAREG2REG  1    0   4   0010 dddD DDSS Ssss   0 0     regD,regA    regA
;Source data must be sign-extended to longword
MOVAREG2REG  1    0   2   0011 dddD DDSS Ssss   0 0     regD,regA    regA

;*** Move memory to memory
MOVMEM2MEM   1    0   4   0010 dddD DDSS Ssss   0 0     indA,indApL,indmAL,indAd16,indAcp,indPCcp,indPCd16,absW,absL    indA,indApL,indmAL,indAd16,indAcp,absW,absL
MOVMEM2MEM   1    0   2   0011 dddD DDSS Ssss   0 0     indA,indApW,indmAW,indAd16,indAcp,indPCcp,indPCd16,absW,absL    indA,indApW,indmAW,indAd16,indAcp,absW,absL
MOVMEM2MEM   1    0   1   0001 dddD DDSS Ssss   0 0     indA,indApB,indmAB,indAd16,indAcp,indPCcp,indPCd16,absW,absL    indA,indApB,indmAB,indAd16,indAcp,absW,absL

;*** Move register to memory
MOVREG2MEM   1    0   4    0010 dddD DDSS Ssss   0 0     regD,regA    indA,indApL,indmAL,indAd16,indAcp,absW,absL
MOVREG2MEM   1    0   2    0011 dddD DDSS Ssss   0 0     regD,regA    indA,indApW,indmAW,indAd16,indAcp,absW,absL
MOVREG2MEM   1    0   1    0001 dddD DDSS Ssss   0 0     regD    indA,indApB,indmAB,indAd16,indAcp,absW,absL

;*** Move memory to register
;move.z x.w,dy
MOVMEM2REG   1    0   4   0010 dddD DDSS Ssss   0 0     indA,indApL,indmAL,indAd16,indAcp,indPCcp,indPCd16,absW,absL    regD
MOVMEM2REG   1    0   2   0011 dddD DDSS Ssss   0 0     indA,indApW,indmAW,indAd16,indAcp,indPCcp,indPCd16,absW,absL    regD
MOVMEM2REG   1    0   1   0001 dddD DDSS Ssss   0 0     indA,indApB,indmAB,indAd16,indAcp,indPCcp,indPCd16,absW,absL    regD

;If the target is an address register, then flags kept unchanged
MOVAMEM2REG  1    0   4   0010 dddD DDSS Ssss   0 0     indA,indApL,indmAL,indAd16,indAcp,indPCcp,indPCd16,absW,absL    regA

;Address registers are always longword-sized, doesn't matter what is the specified size,
;immediate word sized data will be sign-extended to longword
MOVAMEM2REG  1    0   2   0011 dddD DDSS Ssss   0 0     indA,indApW,indmAW,indAd16,indAcp,indPCcp,indPCd16,absW,absL    regA

;*** Load immediate to a register
;move.z #y,dx
MOVIMM2REG   1    0   4   0010 dddD DDSS Ssss   0 0     immedL    regD
MOVIMM2REG   1    0   2   0011 dddD DDSS Ssss   0 0     immedW    regD
MOVIMM2REG   1    0   1   0001 dddD DDSS Ssss   0 0     immedB    regD

;If the target is an address register, then flags kept unchanged
MOVAIMM2REG  1    0   4   0010 dddD DDSS Ssss   0 0     immedL    regA

;Address registers are always longword-sized, doesn't matter what is the specified size,
;immediate word sized data will be sign-extended to longword
MOVAIMM2REG  1    0   2   0011 dddD DDSS Ssss   0 0     immedW    regA

;*** Move quick
MOVEQ        1    0   4   0111 ddd0 ssss ssss   0 0     immedQ    regD

;*** Move multiple registers to memory
MOVEM2MEM    1    1   4   0100 1000 11DD Dddd   0 0     none    indA,indAd16,indAcp,absW,absL
MOVEM2MEM    1    1   2   0100 1000 10DD Dddd   0 0     none    indA,indAd16,indAcp,absW,absL

MOVEM2REG    1    1   4   0100 1100 11SS Ssss   0 0     indA,indAd16,indAcp,indPCd16,indPCcp,absW,absL none
MOVEM2REG    1    1   2   0100 1100 10SS Ssss   0 0     indA,indAd16,indAcp,indPCd16,indPCcp,absW,absL none

;movem.x dx-ay,-(az)
MOVEM2MEMU   1    1   4   0100 1000 1110 0ddd   0 0     none    regA
MOVEM2MEMU   1    1   2   0100 1000 1010 0ddd   0 0     none    regA

;movem.x (az)+,dx-ay
MOVEM2REGU   1    1   4   0100 1100 1101 1sss   0 0     regA    none
MOVEM2REGU   1    1   2   0100 1100 1001 1sss   0 0     regA    none

;*** Move immediate to memory
;move.z #y,mem
MOVIMM2MEM   1    0   4   0010 dddD DDSS Ssss   0 0     immedL    indA,indApL,indmAL,indAd16,indAcp,absW,absL
MOVIMM2MEM   1    0   2   0011 dddD DDSS Ssss   0 0     immedW    indA,indApW,indmAW,indAd16,indAcp,absW,absL
MOVIMM2MEM   1    0   1   0001 dddD DDSS Ssss   0 0     immedB    indA,indApB,indmAB,indAd16,indAcp,absW,absL

;*** Move16 (MC68040 only)
MOV16REG2REGU 0   1  16   1111 0110 0010 0sss   0 0    regA    none
MOV16REG2MEMU 0   0  16   1111 0110 0000 0sss   0 0    regA    immedL
MOV16MEM2REGU 0   0  16   1111 0110 0000 1ddd   0 0    immedL  regA
MOV16REG2MEM  0   0  16   1111 0110 0001 0sss   0 0    regA    immedL
MOV16MEM2REG  0   0  16   1111 0110 0001 1ddd   0 0    immedL  regA

;*** Clear
CLRREG        1   0   4   0100 0010 10DD Dddd   0 0     none    regD
CLRREG        1   0   2   0100 0010 01DD Dddd   0 0     none    regD
CLRREG        1   0   1   0100 0010 00DD Dddd   0 0     none    regD

CLRMEM        1   0   4   0100 0010 10DD Dddd   0 0     none    indA,indApL,indmAL,indAd16,indAcp,absW,absL
CLRMEM        1   0   2   0100 0010 01DD Dddd   0 0     none    indA,indApW,indmAW,indAd16,indAcp,absW,absL
CLRMEM        1   0   1   0100 0010 00DD Dddd   0 0     none    indA,indApB,indmAB,indAd16,indAcp,absW,absL

;*** Load Effective Address
LEAIMM        1   0   4   0100 ddd1 1111 1001   0 0     immedL    regA

;Address registers are always longword-sized, doesn't matter what is the specified size,
;immediate word sized data will be sign-extended to longword
LEAIMM        1   0   2   0100 ddd1 1111 1000   0 0     immedW    regA
LEAIND        1   0   4   0100 ddd1 11SS Ssss   0 0     indA,indPCd16,indAd16,indAcp,indPCcp    regA

;*** Push effective address
PEAIMM        1   0   4   0100 1000 0111 1001   0 0     immedL    none
PEAIMM        1   0   2   0100 1000 0111 1000   0 0     immedW    none
PEAIND        1   0   4   0100 1000 01SS Ssss   0 0     indA,indPCd16,indAd16,indAcp,indPCcp    none

;---------------------------------------------------------------------
;*** Special data moving instructions
;*** Set Conditional
STREG         1   0   1   0101 0000 11DD Dddd   0 0     none    regD
SFREG         1   0   1   0101 0001 11DD Dddd   0 0     none    regD
SCCREG        1   0   1   0101 Ssss 11DD Dddd   0 0     CC_cc,CC_cs,CC_eq,CC_ge,CC_gt,CC_hi,CC_le,CC_ls,CC_lt,CC_mi,CC_ne,CC_pl,CC_vc,CC_vs   regD

STMEM         1   0   1   0101 0000 11DD Dddd   0 0     none    indA,indApB,indmAB,indAd16,indAcp,absW,absL
SFMEM         1   0   1   0101 0001 11DD Dddd   0 0     none    indA,indApB,indmAB,indAd16,indAcp,absW,absL
SCCMEM        1   0   1   0101 Ssss 11DD Dddd   0 0     CC_cc,CC_cs,CC_eq,CC_ge,CC_gt,CC_hi,CC_le,CC_ls,CC_lt,CC_mi,CC_ne,CC_pl,CC_vc,CC_vs   indA,indApB,indmAB,indAd16,indAcp,absW,absL

;*** Condition Code Register moving
MOVCCR2REG    1   0   2   0100 0010 11DD Dddd   0 0     none    regD
MOVCCR2MEM    1   0   2   0100 0010 11DD Dddd   0 0     none    indA,indApW,indmAW,indAd16,indAcp,absW,absL

MOVIMM2CCR    1   0   2   0100 0100 11SS Ssss   0 0     immedW  none
MOVREG2CCR    1   0   2   0100 0100 11SS Ssss   0 0     regD    none
MOVMEM2CCR    1   0   2   0100 0100 11SS Ssss   0 0     indA,indApW,indmAW,indAd16,indAcp,indPCcp,indPCd16,absW,absL    none

;---------------------------------------------------------------------
;*** Branch instructions
;*** DBcc
DBCOND        1   1   2   0101 Ssss 1100 1ddd   1 0     CC_cc,CC_cs,CC_eq,CC_ge,CC_gt,CC_hi,CC_le,CC_ls,CC_lt,CC_mi,CC_ne,CC_pl,CC_vc,CC_vs    regD
DBF           1   0   2   0101 0001 1100 1ddd   1 0     immedW    regD

;*** Bcc.b
;Must precede Bcc.w and Bcc.l
BCOND         1   0   1   0110 Ssss dddd dddd   1 0     CC_cc,CC_cs,CC_eq,CC_ge,CC_gt,CC_hi,CC_le,CC_ls,CC_lt,CC_mi,CC_ne,CC_pl,CC_vc,CC_vs    immedQ
BRA           1   0   1   0110 0000 ssss ssss   1 1     immedQ    none
BSR           1   0   1   0110 0001 ssss ssss   1 1     immedQ    none

;*** Bcc.w
BCOND         1   0   2   0110 Ssss 0000 0000   1 0     CC_cc,CC_cs,CC_eq,CC_ge,CC_gt,CC_hi,CC_le,CC_ls,CC_lt,CC_mi,CC_ne,CC_pl,CC_vc,CC_vs    immedW
BRA           1   0   2   0110 0000 0000 0000   1 1     immedW    none
BSR           1   0   2   0110 0001 0000 0000   1 1     immedW    none

;*** Bcc.l
BCOND         1   0   4   0110 Ssss 1111 1111   1 0     CC_cc,CC_cs,CC_eq,CC_ge,CC_gt,CC_hi,CC_le,CC_ls,CC_lt,CC_mi,CC_ne,CC_pl,CC_vc,CC_vs    immedL
BRA           1   0   4   0110 0000 1111 1111   1 1     immedL    none
BSR           1   0   4   0110 0001 1111 1111   1 1     immedL    none

;*** Jump
;JMPIMM modified to take argument directly from address translation
;original source addressing is absL
JMPIMM        1   0   4   0100 1110 1111 1001   1 1     immedL    none
JMPIND        1   0   4   0100 1110 11SS Ssss   1 0     indAd16,indPCd16,indAd16,indAcp,indPCcp,indA    none

;JSRIMM modified to take argument directly from address translation
;original source addressing is absL
JSRIMM        1   0   4   0100 1110 1011 1001   1 1     immedL    none
JSRIND        1   0   4   0100 1110 10SS Ssss   1 0     indAd16,indPCd16,indAcp,indPCcp,indA    none

RTS           1   0   0   0100 1110 0111 0101   1 0     none    none
RTD           1   0   2   0100 1110 0111 0100   1 0     immedW    none
RTR           1   0   0   0100 1110 0111 0111   1 0     none    none

;---------------------------------------------------------------------
;*** Logic instructions
;*** Exclusive or
EORIMM2REG    1   0   4   0000 1010 10DD Dddd   0 0     immedL    regD
EORIMM2REG    1   0   2   0000 1010 01DD Dddd   0 0     immedW    regD
EORIMM2REG    1   0   1   0000 1010 00DD Dddd   0 0     immedB    regD

EORIMM2MEM    1   0   4   0000 1010 10DD Dddd   0 0     immedL    indA,indApL,indmAL,indAd16,indAcp,absW,absL
EORIMM2MEM    1   0   2   0000 1010 01DD Dddd   0 0     immedW    indA,indApW,indmAW,indAd16,indAcp,absW,absL
EORIMM2MEM    1   0   1   0000 1010 00DD Dddd   0 0     immedB    indA,indApB,indmAB,indAd16,indAcp,absW,absL

EORREG2REG    1   0   4   1011 sss1 10DD Dddd   0 0     regD    regD
EORREG2REG    1   0   2   1011 sss1 01DD Dddd   0 0     regD    regD
EORREG2REG    1   0   1   1011 sss1 00DD Dddd   0 0     regD    regD

EORREG2MEM    1   0   4   1011 sss1 10DD Dddd   0 0     regD    indA,indApL,indmAL,indAd16,indAcp,absW,absL
EORREG2MEM    1   0   2   1011 sss1 01DD Dddd   0 0     regD    indA,indApW,indmAW,indAd16,indAcp,absW,absL
EORREG2MEM    1   0   1   1011 sss1 00DD Dddd   0 0     regD    indA,indApB,indmAB,indAd16,indAcp,absW,absL

;*** And
;ANDI.x #imm,Dy
ANDIMM2REG    1   0   4   0000 0010 10DD Dddd   0 0     immedL    regD
ANDIMM2REG    1   0   2   0000 0010 01DD Dddd   0 0     immedW    regD
ANDIMM2REG    1   0   1   0000 0010 00DD Dddd   0 0     immedB    regD

ANDIMM2MEM    1   0   4   0000 0010 10DD Dddd   0 0     immedL    indA,indApL,indmAL,indAd16,indAcp,absW,absL
ANDIMM2MEM    1   0   2   0000 0010 01DD Dddd   0 0     immedW    indA,indApW,indmAW,indAd16,indAcp,absW,absL
ANDIMM2MEM    1   0   1   0000 0010 00DD Dddd   0 0     immedB    indA,indApB,indmAB,indAd16,indAcp,absW,absL

ANDREG2REG    1   0   4   1100 ddd0 10SS Ssss   0 0     regD,regA    regD
ANDREG2REG    1   0   2   1100 ddd0 01SS Ssss   0 0     regD,regA    regD
ANDREG2REG    1   0   1   1100 ddd0 00SS Ssss   0 0     regD,regA    regD

ANDREG2MEM    1   0   4   1100 sss1 10DD Dddd   0 0     regD    indA,indApL,indmAL,indAd16,indAcp,absW,absL
ANDREG2MEM    1   0   2   1100 sss1 01DD Dddd   0 0     regD    indA,indApW,indmAW,indAd16,indAcp,absW,absL
ANDREG2MEM    1   0   1   1100 sss1 00DD Dddd   0 0     regD    indA,indApB,indmAB,indAd16,indAcp,absW,absL

ANDMEM2REG    1   0   4   1100 ddd0 10SS Ssss   0 0     indA,indApL,indmAL,indAd16,indAcp,indPCcp,indPCd16,absW,absL    regD
ANDMEM2REG    1   0   2   1100 ddd0 01SS Ssss   0 0     indA,indApW,indmAW,indAd16,indAcp,indPCcp,indPCd16,absW,absL    regD
ANDMEM2REG    1   0   1   1100 ddd0 00SS Ssss   0 0     indA,indApB,indmAB,indAd16,indAcp,indPCcp,indPCd16,absW,absL    regD

;AND.x #imm,Dy
ANDIMM2REG    1   0   4   1100 ddd0 10SS Ssss   0 0     immedL    regD
ANDIMM2REG    1   0   2   1100 ddd0 01SS Ssss   0 0     immedW    regD
ANDIMM2REG    1   0   1   1100 ddd0 00SS Ssss   0 0     immedB    regD

;*** Or
;ORI #imm
ORIMM2REG     1   0   4   0000 0000 10DD Dddd   0 0     immedL    regD
ORIMM2REG     1   0   2   0000 0000 01DD Dddd   0 0     immedW    regD
ORIMM2REG     1   0   1   0000 0000 00DD Dddd   0 0     immedB    regD

ORIMM2MEM     1   0   4   0000 0000 10DD Dddd   0 0     immedL    indA,indApL,indmAL,indAd16,indAcp,absW,absL
ORIMM2MEM     1   0   2   0000 0000 01DD Dddd   0 0     immedW    indA,indApW,indmAW,indAd16,indAcp,absW,absL
ORIMM2MEM     1   0   1   0000 0000 00DD Dddd   0 0     immedB    indA,indApB,indmAB,indAd16,indAcp,absW,absL

ORREG2REG     1   0   4   1000 ddd0 10SS Ssss   0 0     regD,regA    regD
ORREG2REG     1   0   2   1000 ddd0 01SS Ssss   0 0     regD,regA    regD
ORREG2REG     1   0   1   1000 ddd0 00SS Ssss   0 0     regD,regA    regD

ORREG2MEM     1   0   4   1000 sss1 10DD Dddd   0 0     regD    indA,indApL,indmAL,indAd16,indAcp,absW,absL
ORREG2MEM     1   0   2   1000 sss1 01DD Dddd   0 0     regD    indA,indApW,indmAW,indAd16,indAcp,absW,absL
ORREG2MEM     1   0   1   1000 sss1 00DD Dddd   0 0     regD    indA,indApB,indmAB,indAd16,indAcp,absW,absL

ORMEM2REG     1   0   4   1000 ddd0 10SS Ssss   0 0     indA,indApL,indmAL,indAd16,indAcp,indPCcp,indPCd16,absW,absL    regD
ORMEM2REG     1   0   2   1000 ddd0 01SS Ssss   0 0     indA,indApW,indmAW,indAd16,indAcp,indPCcp,indPCd16,absW,absL    regD
ORMEM2REG     1   0   1   1000 ddd0 00SS Ssss   0 0     indA,indApB,indmAB,indAd16,indAcp,indPCcp,indPCd16,absW,absL    regD

;OR #imm
ORIMM2REG     1   0   4   1000 ddd0 10SS Ssss   0 0     immedL    regD
ORIMM2REG     1   0   2   1000 ddd0 01SS Ssss   0 0     immedW    regD
ORIMM2REG     1   0   1   1000 ddd0 00SS Ssss   0 0     immedB    regD

;*** Not
NOTREG        1   0   4   0100 0110 10DD Dddd   0 0     none    regD
NOTREG        1   0   2   0100 0110 01DD Dddd   0 0     none    regD
NOTREG        1   0   1   0100 0110 00DD Dddd   0 0     none    regD

NOTMEM        1   0   4   0100 0110 10DD Dddd   0 0     none    indA,indApL,indmAL,indAd16,indAcp,absW,absL
NOTMEM        1   0   2   0100 0110 01DD Dddd   0 0     none    indA,indApW,indmAW,indAd16,indAcp,absW,absL
NOTMEM        1   0   1   0100 0110 00DD Dddd   0 0     none    indA,indApB,indmAB,indAd16,indAcp,absW,absL

;*** Condition Code Register bit logic
ANDIMM2CCR    1   0   1   0000 0010 0011 1100   0 0     immedB    none
ORIMM2CCR     1   0   1   0000 0000 0011 1100   0 0     immedB    none
EORIMM2CCR    1   0   1   0000 1010 0011 1100   0 0     immedB    none

;*** Btst
BTSTIMM2MEM   1   0   1   0000 1000 00DD Dddd   0 0     immedW  indA,indApB,indmAB,indAd16,indAcp,indPCcp,absW,absL,indPCd16
BTSTREG2MEM   1   0   1   0000 sss1 00DD Dddd   0 0     regD    indA,indApB,indmAB,indAd16,indAcp,indPCcp,absW,absL,indPCd16
BTSTIMM2REG   1   0   4   0000 1000 00DD Dddd   0 0     immedW  regD
BTSTREG2REG   1   0   4   0000 sss1 00DD Dddd   0 0     regD    regD
BTSTREG2IMM   1   0   1   0000 sss1 00DD Dddd   0 0     regD    immedB

;*** Bset
BSETIMM2MEM   1   0   1   0000 1000 11DD Dddd   0 0     immedW    indA,indApB,indmAB,indAd16,indAcp,absW,absL
BSETREG2MEM   1   0   1   0000 sss1 11DD Dddd   0 0     regD    indA,indApB,indmAB,indAd16,indAcp,absW,absL
BSETIMM2REG   1   0   4   0000 1000 11DD Dddd   0 0     immedW    regD
BSETREG2REG   1   0   4   0000 sss1 11DD Dddd   0 0     regD    regD

;*** Bclr
BCLRIMM2MEM   1   0   1   0000 1000 10DD Dddd   0 0     immedW    indA,indApB,indmAB,indAd16,indAcp,absW,absL
