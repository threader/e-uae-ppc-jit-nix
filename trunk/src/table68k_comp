;immediate word sized data will be sign-extended to longword
SUBAQ2REGL   0   0101 sss1 01DD Dddd   0 0     immedQ    regA

SUBQ2MEML    0   0101 sss1 10DD Dddd   0 0     immedQ    indA,indApL,indmAL,indAd16,indAcp,absW,absL
SUBQ2MEMW    0   0101 sss1 01DD Dddd   0 0     immedQ    indA,indApW,indmAW,indAd16,indAcp,absW,absL
SUBQ2MEMB    0   0101 sss1 00DD Dddd   0 0     immedQ    indA,indApB,indmAB,indAd16,indAcp,absW,absL

;*** SUBA
;If the target is an address register, then flags kept unchanged
SUBAMEM2REGL 0   1001 ddd1 11SS Ssss   0 0     indA,indApL,indmAL,indPCd16,indAd16,indAcp,indPCcp,absW,absL    regA

;Address registers are always longword-sized, doesn't matter what is the specified size,
;immediate word sized data will be sign-extended to longword
SUBAMEM2REGW 0   1001 ddd0 11SS Ssss   0 0     indA,indApW,indmAW,indPCd16,indAd16,indAcp,indPCcp,absW,absL    regA
SUBAIMM2REGL 0   1001 ddd1 11SS Ssss   0 0     immedL    regA
SUBAIMM2REGW 0   1001 ddd0 11SS Ssss   0 0     immedW    regA
SUBAREG2REGL 0   1001 ddd1 11SS Ssss   0 0     regA,regD    regA
SUBAREG2REGW 0   1001 ddd0 11SS Ssss   0 0     regA,regD    regA

;*** SUBX
SUBXREG2REGL 0   1001 ddd1 1000 0sss   0 0     regD    regD
SUBXREG2REGW 0   1001 ddd1 0100 0sss   0 0     regD    regD
SUBXREG2REGB 0   1001 ddd1 0000 0sss   0 0     regD    regD

SUBXMEM2MEML 0   1001 ddd1 1000 1sss   0 0     indmAL    indmAL
SUBXMEM2MEMW 0   1001 ddd1 0100 1sss   0 0     indmAW    indmAW
SUBXMEM2MEMB 0   1001 ddd1 0000 1sss   0 0     indmAB    indmAB

;*** MULS
MULSIMM2REGW 0   1100 ddd1 11SS Ssss   0 0     immedW    regD
MULSREG2REGW 0   1100 ddd1 11SS Ssss   0 0     regD    regD
MULSMEM2REGW 0   1100 ddd1 11SS Ssss   0 0     indA,indApW,indmAW,indAd16,indAcp,indPCcp,indPCd16,absW,absL    regD

;*** MULU
MULUIMM2REGW 0   1100 ddd0 11SS Ssss   0 0     immedW    regD
MULUREG2REGW 0   1100 ddd0 11SS Ssss   0 0     regD    regD
MULUMEM2REGW 0   1100 ddd0 11SS Ssss   0 0     indA,indApW,indmAW,indAd16,indAcp,indPCcp,indPCd16,absW,absL    regD

;*** MULU.L/MULS.L
MULIMM2REGL  0   0100 1100 0011 1100   0 0     none    none
MULREG2REGL  0   0100 1100 00SS Ssss   0 0     regD    none
MULMEM2REGL  0   0100 1100 00SS Ssss   0 0     indA,indApL,indmAL,indAd16,indAcp,indPCcp,indPCd16,absW,absL    none

;*** DIVS
DIVSIMM2REGW 0   1000 ddd1 11SS Ssss   0 0     immedW    regD
DIVSREG2REGW 0   1000 ddd1 11SS Ssss   0 0     regD    regD
DIVSMEM2REGW 0   1000 ddd1 11SS Ssss   0 0     indA,indApW,indmAW,indAd16,indAcp,indPCcp,indPCd16,absW,absL    regD

;*** DIVU
