;***
;*** E-UAE JIT compiling
;***
;*** Opcode tables
;***

;*** Format:
;*** opcode_name implemented binary_code_in_4_parts jump constjump flagsin flagsout srcaddr destaddr
;***   implemented - this instruction was implemented or not (1/0)
;***   binary code - contains "S" and "s" as source addr/reg, 
;***                          "D" and "d" as destination bits addr/reg
;***                          "x" for ignoring that bit
;***   jump - compile stopping opcode (branch mostly) (1/0)
;***   constjump - constant jump opcode (1/0)
;***   flagsin - flags required (1/0, XNZVC)
;***   flagsout - flags overwritten (1/0, XNZVC)
;***   srcaddr - source addressing
;***   destaddr - destination addressing
;***
;*** Addressing markups:
;***
;*** regD    - Dx register: Dx
;*** regA    - Ax register: Ax
;*** indA    - Ax indirect: (Ax)
;*** indmAy  - Ax indirect with predecrement : -(Ax), y - size: B, W, L
;*** indmAyk - Ax indirect with predecrement : -(Ax), y - size: B, W, L (keeps original register value in _SRC)
;*** indApy  - Ax indirect with postincrement: (Ax)+, y - size: B, W, L
;*** indApyk - Ax indirect with postincrement: (Ax)+, y - size: B, W, L (keeps original register value in _SRC)
;*** immed   - immediate value: #x - size: B, W, L
;*** immedQ  - immediate value in opcode
;*** indAd16 - Ax indirect d16 indexed: d16(Ax)
;*** indPCd16 - PC indirect d16 indexed: d16(PC)
;*** absW    - absolute word sized address: x.w
;*** absL    - absolute longword sized address: x.l
;*** indAcp  - Ax indirect with complex displacement, eg: ([bd, An] Rn, od)
;*** indPCcp - PC indirect with complex displacement, eg: ([bd, PC] Rn, od)

#Addressing
;address   modus  register
;none should be the first int the list
none        xxx    xxx
regD        000    rrr
regA        001    rrr
indA        010    rrr
indmAL      100    rrr
indmAW      100    rrr
indmAB      100    rrr
indApL      011    rrr
indApW      011    rrr
indApB      011    rrr
indmALk     100    rrr
indmAWk     100    rrr
indmABk     100    rrr
indApLk     011    rrr
indApWk     011    rrr
indApBk     011    rrr
immedL      111    100
immedW      111    100
immedB      111    100
immedQ      rrr    rrr
indAd16     101    rrr
indPCd16    111    010
absW        111    000
absL        111    001
indAcp      110    rrr
indPCcp     111    011

;Conditional codes
CC_cc   0    100
CC_cs   0    101
CC_eq   0    111
CC_ge   1    100
CC_gt   1    110
CC_hi   0    010
CC_le   1    111
CC_ls   0    011
CC_lt   1    101
CC_mi   1    011
CC_ne   0    110
CC_pl   1    010
CC_vc   1    000
CC_vs   1    001
CC_t    0    000
CC_f    0    001

;FPU Conditional codes
FCC_f      000    000
FCC_eq     000    001
FCC_ogt    000    010
FCC_oge    000    011
FCC_olt    000    100
FCC_ole    000    101
FCC_ogl    000    110
FCC_or     000    111
FCC_un     001    000
FCC_ueq    001    001
FCC_ugt    001    010
FCC_uge    001    011
FCC_ult    001    100
FCC_ule    001    101
FCC_ne     001    110
FCC_t      001    111
FCC_sf     010    000
FCC_seq    010    001
FCC_gt     010    010
FCC_ge     010    011
FCC_lt     010    100
FCC_le     010    101
FCC_gl     010    110
FCC_gle    010    111
FCC_ngle   011    000
FCC_ngl    011    001
FCC_nle    011    010
FCC_nlt    011    011
FCC_nge    011    100
FCC_ngt    011    101
FCC_sne    011    110
FCC_st     011    111

#Opcodes
;Format:
;name      impl  bincode            jump|cjmp  srcaddr destaddr
;---------------------------------------------------------------------
;*** Data moving instructions
;*** Move register-register
MOVREG2REGL  1   0010 dddD DDSS Ssss   0 0     regD,regA    regD
MOVREG2REGW  1   0011 dddD DDSS Ssss   0 0     regD,regA    regD
MOVREG2REGB  1   0001 dddD DDSS Ssss   0 0     regD     regD

;If the target is an address register, then flags kept unchanged
MOVAREG2REGL 1   0010 dddD DDSS Ssss   0 0     regD,regA    regA
MOVAREG2REGW 1   0011 dddD DDSS Ssss   0 0     regD,regA    regA

;*** Move memory to memory
MOVMEM2MEML  1   0010 dddD DDSS Ssss   0 0     indA,indApL,indmAL,indAd16,indAcp,indPCcp,indPCd16,absW,absL    indA,indApL,indmAL,indAd16,indAcp,absW,absL
MOVMEM2MEMW  1   0011 dddD DDSS Ssss   0 0     indA,indApW,indmAW,indAd16,indAcp,indPCcp,indPCd16,absW,absL    indA,indApW,indmAW,indAd16,indAcp,absW,absL
MOVMEM2MEMB  1   0001 dddD DDSS Ssss   0 0     indA,indApB,indmAB,indAd16,indAcp,indPCcp,indPCd16,absW,absL    indA,indApB,indmAB,indAd16,indAcp,absW,absL

;*** Move register to memory
MOVREG2MEML  1   0010 dddD DDSS Ssss   0 0     regD,regA    indA,indApL,indmAL,indAd16,indAcp,absW,absL
MOVREG2MEMW  1   0011 dddD DDSS Ssss   0 0     regD,regA    indA,indApW,indmAW,indAd16,indAcp,absW,absL
MOVREG2MEMB  1   0001 dddD DDSS Ssss   0 0     regD    indA,indApB,indmAB,indAd16,indAcp,absW,absL

;*** Move reg to mem with the same register
;a0
MOVREG2MEML  1   0010 000D DDSS S000   0 0     regA    indApLk,indmALk
;a1
MOVREG2MEML  1   0010 001D DDSS S001   0 0     regA    indApLk,indmALk
;a2
MOVREG2MEML  1   0010 010D DDSS S010   0 0     regA    indApLk,indmALk
;a3
MOVREG2MEML  1   0010 011D DDSS S011   0 0     regA    indApLk,indmALk
;a4
MOVREG2MEML  1   0010 100D DDSS S100   0 0     regA    indApLk,indmALk
;a5
MOVREG2MEML  1   0010 101D DDSS S101   0 0     regA    indApLk,indmALk
;a6
MOVREG2MEML  1   0010 110D DDSS S110   0 0     regA    indApLk,indmALk
;a7
MOVREG2MEML  1   0010 111D DDSS S111   0 0     regA    indApLk,indmALk

;a0
MOVREG2MEMW  1   0011 000D DDSS S000   0 0     regA    indApWk,indmAWk
;a1
MOVREG2MEMW  1   0011 001D DDSS S001   0 0     regA    indApWk,indmAWk
;a2
MOVREG2MEMW  1   0011 010D DDSS S010   0 0     regA    indApWk,indmAWk
;a3
MOVREG2MEMW  1   0011 011D DDSS S011   0 0     regA    indApWk,indmAWk
;a4
MOVREG2MEMW  1   0011 100D DDSS S100   0 0     regA    indApWk,indmAWk
;a5
MOVREG2MEMW  1   0011 101D DDSS S101   0 0     regA    indApWk,indmAWk
;a6
MOVREG2MEMW  1   0011 110D DDSS S110   0 0     regA    indApWk,indmAWk
;a7
MOVREG2MEMW  1   0011 111D DDSS S111   0 0     regA    indApWk,indmAWk

;*** Move memory to register
;move.z x.w,dy
MOVMEM2REGL  1   0010 dddD DDSS Ssss   0 0     indA,indApL,indmAL,indAd16,indAcp,indPCcp,indPCd16,absW,absL    regD
MOVMEM2REGW  1   0011 dddD DDSS Ssss   0 0     indA,indApW,indmAW,indAd16,indAcp,indPCcp,indPCd16,absW,absL    regD
MOVMEM2REGB  1   0001 dddD DDSS Ssss   0 0     indA,indApB,indmAB,indAd16,indAcp,indPCcp,indPCd16,absW,absL    regD

;If the target is an address register, then flags kept unchanged
MOVAMEM2REGL 1   0010 dddD DDSS Ssss   0 0     indA,indApL,indmAL,indAd16,indAcp,indPCcp,indPCd16,absW,absL    regA

;Address registers are always longword-sized, doesn't matter what is the specified size,
;immediate word sized data will be sign-extended to longword
MOVAMEM2REGW 1   0011 dddD DDSS Ssss   0 0     indA,indApW,indmAW,indAd16,indAcp,indPCcp,indPCd16,absW,absL    regA

;*** Load immediate to a register
;move.z #y,dx
MOVIMM2REGL  1   0010 dddD DDSS Ssss   0 0     immedL    regD
MOVIMM2REGW  1   0011 dddD DDSS Ssss   0 0     immedW    regD
MOVIMM2REGB  1   0001 dddD DDSS Ssss   0 0     immedB    regD

;If the target is an address register, then flags kept unchanged
MOVAIMM2REGL 1   0010 dddD DDSS Ssss   0 0     immedL    regA

;Address registers are always longword-sized, doesn't matter what is the specified size,
;immediate word sized data will be sign-extended to longword
MOVAIMM2REGW 1   0011 dddD DDSS Ssss   0 0     immedW    regA

;*** Move quick
MOVEQ        1   0111 ddd0 ssss ssss   0 0     immedQ    regD

;*** Moveregs to mem
MOVEM2MEML   0   0100 1000 11DD Dddd   0 0     none    indA,indAd16,indAcp,absW,absL
MOVEM2MEMW   0   0100 1000 10DD Dddd   0 0     none    indA,indAd16,indAcp,absW,absL

MOVEM2REGL   0   0100 1100 11SS Ssss   0 0     indA,indAd16,indAcp,indPCd16,indPCcp,absW,absL none
MOVEM2REGW   0   0100 1100 10SS Ssss   0 0     indA,indAd16,indAcp,indPCd16,indPCcp,absW,absL none

;movem.x dx-ay,-(az)
MOVEM2MEMUL  0   0100 1000 1110 0ddd   0 0     none    regA
MOVEM2MEMUW  0   0100 1000 1010 0ddd   0 0     none    regA

;movem.x (az)+,dx-ay
MOVEM2REGUL  0   0100 1100 1101 1sss   0 0     regA    none
MOVEM2REGUW  0   0100 1100 1001 1sss   0 0     regA    none

;*** Move immediate to mem
;move.z #y,dx
MOVIMM2MEML  0   0010 dddD DDSS Ssss   0 0     immedL    indA,indApL,indmAL,indAd16,indAcp,absW,absL
MOVIMM2MEMW  0   0011 dddD DDSS Ssss   0 0     immedW    indA,indApW,indmAW,indAd16,indAcp,absW,absL
MOVIMM2MEMB  0   0001 dddD DDSS Ssss   0 0     immedB    indA,indApB,indmAB,indAd16,indAcp,absW,absL

;*** Move16
MOV16REG2REGU 0   1111 0110 0010 0sss   0 0    regA    none
MOV16REG2MEMU 0   1111 0110 0000 0sss   0 0    regA    immedL
MOV16MEM2REGU 0   1111 0110 0000 1ddd   0 0    immedL  regA
MOV16REG2MEM  0   1111 0110 0001 0sss   0 0    regA    immedL
MOV16MEM2REG  0   1111 0110 0001 1ddd   0 0    immedL  regA

;*** Clear
CLRREGL      0   0100 0010 10DD Dddd   0 0     none    regD
CLRREGW      0   0100 0010 01DD Dddd   0 0     none    regD
CLRREGB      0   0100 0010 00DD Dddd   0 0     none    regD

CLRMEML      0   0100 0010 10DD Dddd   0 0     none    indA,indApL,indmAL,indAd16,indAcp,absW,absL
CLRMEMW      0   0100 0010 01DD Dddd   0 0     none    indA,indApW,indmAW,indAd16,indAcp,absW,absL
CLRMEMB      0   0100 0010 00DD Dddd   0 0     none    indA,indApB,indmAB,indAd16,indAcp,absW,absL

;*** Load Effective Address
LEAIMML      0   0100 ddd1 1111 1001   0 0     immedL    regA

;Address registers are always longword-sized, doesn't matter what is the specified size,
;immediate word sized data will be sign-extended to longword
LEAIMMW      0   0100 ddd1 1111 1000   0 0     immedW    regA
LEAIND       0   0100 ddd1 11SS Ssss   0 0     indA,indPCd16,indAd16,indAcp,indPCcp    regA

;*** Push effective address
PEAIMML      0   0100 1000 0111 1001   0 0     immedL    none
PEAIMMW      0   0100 1000 0111 1000   0 0     immedW    none
PEAIND       0   0100 1000 01SS Ssss   0 0     indA,indPCd16,indAd16,indAcp,indPCcp    none

;*** Move Peripherials
MOVPREG2MEML 0   0000 sss1 1100 1ddd   0 0     regD    indAd16
MOVPREG2MEMW 0   0000 sss1 1000 1ddd   0 0     regD    indAd16
MOVPMEM2REGL 0   0000 ddd1 0100 1sss   0 0     indAd16    regD
MOVPMEM2REGW 0   0000 ddd1 0000 1sss   0 0     indAd16    regD

;---------------------------------------------------------------------
;*** Special data moving instructions
;*** Set Conditional
STREGB       0   0101 0000 11DD Dddd   0 0     none    regD
SFREGB       0   0101 0001 11DD Dddd   0 0     none    regD
SCCREGB      0   0101 Ssss 11DD Dddd   0 0     CC_cc   regD
SCCREGB      0   0101 Ssss 11DD Dddd   0 0     CC_cs   regD
SCCREGB      0   0101 Ssss 11DD Dddd   0 0     CC_eq   regD
SCCREGB      0   0101 Ssss 11DD Dddd   0 0     CC_ge   regD
SCCREGB      0   0101 Ssss 11DD Dddd   0 0     CC_gt   regD
SCCREGB      0   0101 Ssss 11DD Dddd   0 0     CC_hi   regD
SCCREGB      0   0101 Ssss 11DD Dddd   0 0     CC_le   regD
SCCREGB      0   0101 Ssss 11DD Dddd   0 0     CC_ls   regD
SCCREGB      0   0101 Ssss 11DD Dddd   0 0     CC_lt   regD
SCCREGB      0   0101 Ssss 11DD Dddd   0 0     CC_mi   regD
SCCREGB      0   0101 Ssss 11DD Dddd   0 0     CC_ne   regD
SCCREGB      0   0101 Ssss 11DD Dddd   0 0     CC_pl   regD
SCCREGB      0   0101 Ssss 11DD Dddd   0 0     CC_vc   regD
SCCREGB      0   0101 Ssss 11DD Dddd   0 0     CC_vs   regD

STMEMB       0   0101 0000 11DD Dddd   0 0     none    indA,indApB,indmAB,indAd16,indAcp,absW,absL
SFMEMB       0   0101 0001 11DD Dddd   0 0     none    indA,indApB,indmAB,indAd16,indAcp,absW,absL
SCCMEMB      0   0101 Ssss 11DD Dddd   0 0     CC_cc   indA,indApB,indmAB,indAd16,indAcp,absW,absL
SCCMEMB      0   0101 Ssss 11DD Dddd   0 0     CC_cs   indA,indApB,indmAB,indAd16,indAcp,absW,absL
SCCMEMB      0   0101 Ssss 11DD Dddd   0 0     CC_eq   indA,indApB,indmAB,indAd16,indAcp,absW,absL
SCCMEMB      0   0101 Ssss 11DD Dddd   0 0     CC_ge   indA,indApB,indmAB,indAd16,indAcp,absW,absL
SCCMEMB      0   0101 Ssss 11DD Dddd   0 0     CC_gt   indA,indApB,indmAB,indAd16,indAcp,absW,absL
SCCMEMB      0   0101 Ssss 11DD Dddd   0 0     CC_hi   indA,indApB,indmAB,indAd16,indAcp,absW,absL
SCCMEMB      0   0101 Ssss 11DD Dddd   0 0     CC_le   indA,indApB,indmAB,indAd16,indAcp,absW,absL
SCCMEMB      0   0101 Ssss 11DD Dddd   0 0     CC_ls   indA,indApB,indmAB,indAd16,indAcp,absW,absL
SCCMEMB      0   0101 Ssss 11DD Dddd   0 0     CC_lt   indA,indApB,indmAB,indAd16,indAcp,absW,absL
SCCMEMB      0   0101 Ssss 11DD Dddd   0 0     CC_mi   indA,indApB,indmAB,indAd16,indAcp,absW,absL
SCCMEMB      0   0101 Ssss 11DD Dddd   0 0     CC_ne   indA,indApB,indmAB,indAd16,indAcp,absW,absL
SCCMEMB      0   0101 Ssss 11DD Dddd   0 0     CC_pl   indA,indApB,indmAB,indAd16,indAcp,absW,absL
SCCMEMB      0   0101 Ssss 11DD Dddd   0 0     CC_vc   indA,indApB,indmAB,indAd16,indAcp,absW,absL
SCCMEMB      0   0101 Ssss 11DD Dddd   0 0     CC_vs   indA,indApB,indmAB,indAd16,indAcp,absW,absL

;*** Condition Code Register moving
MOVCCR2REGW  0   0100 0010 11DD Dddd   0 0     none    regD
MOVCCR2MEMW  0   0100 0010 11DD Dddd   0 0     none    indA,indApW,indmAW,indAd16,indAcp,absW,absL

MOVIMM2CCRW  0   0100 0100 11SS Ssss   0 0     immedW  none
MOVREG2CCRW  0   0100 0100 11SS Ssss   0 0     regD    none
MOVMEM2CCRW  0   0100 0100 11SS Ssss   0 0     indA,indApW,indmAW,indAd16,indAcp,indPCcp,indPCd16,absW,absL    none

;*** Status Register moving
MOVSR2REGW   0   0100 0000 11DD Dddd   0 0     none    regD
MOVSR2MEMW   0   0100 0000 11DD Dddd   0 0     none    indA,indApW,indmAW,indAd16,indAcp,absW,absL

MOVIMM2SRW   0   0100 0110 11SS Ssss   0 0     immedW  none
MOVREG2SRW   0   0100 0110 11SS Ssss   0 0     regD    none
MOVMEM2SRW   0   0100 0110 11SS Ssss   0 0     indA,indApW,indmAW,indAd16,indAcp,indPCcp,indPCd16,absW,absL    none

;*** Move USP
MOVUSP2REGL  0   0100 1110 0110 1ddd   0 0     none    regA
MOVREG2USPL  0   0100 1110 0110 0sss   0 0     regA    regA

;*** Move control register
MOVCREG2CTRL 0   0100 1110 0111 1011   0 0     none    none
MOVCCTR2REGL 0   0100 1110 0111 1010   0 0     none    none

;---------------------------------------------------------------------
;*** Branch instructions
;*** DBcc
DBCOND       0   0101 Ssss 1100 1ddd   1 0     CC_cc,CC_cs,CC_eq,CC_ge,CC_gt,CC_hi,CC_le,CC_ls,CC_lt,CC_mi,CC_ne,CC_pl,CC_vc,CC_vs    regD
DBF          0   0101 0001 1100 1ddd   1 0     none    regD

;*** Bcc.b
;Must precede Bcc.w and Bcc.l
BCONDB       0   0110 Ssss dddd dddd   1 0     CC_cc,CC_cs,CC_eq,CC_ge,CC_gt,CC_hi,CC_le,CC_ls,CC_lt,CC_mi,CC_ne,CC_pl,CC_vc,CC_vs    immedQ
BRAB         0   0110 0000 dddd dddd   1 1     none    immedQ
BSRB         0   0110 0001 dddd dddd   1 1     none    immedQ

;*** Bcc.w
BCONDW       0   0110 Ssss 0000 0000   1 0     CC_cc,CC_cs,CC_eq,CC_ge,CC_gt,CC_hi,CC_le,CC_ls,CC_lt,CC_mi,CC_ne,CC_pl,CC_vc,CC_vs    none
BRAW         0   0110 0000 0000 0000   1 1     none    none
BSRW         0   0110 0001 0000 0000   1 1     none    none

;*** Bcc.l
BCONDL       0   0110 Ssss 1111 1111   1 0     CC_cc,CC_cs,CC_eq,CC_ge,CC_gt,CC_hi,CC_le,CC_ls,CC_lt,CC_mi,CC_ne,CC_pl,CC_vc,CC_vs    none
BRAL         0   0110 0000 1111 1111   1 1     none    none
BSRL         0   0110 0001 1111 1111   1 1     none    none

;*** Jump
;JMPIMM modified to take argument directly from address translation
;original source addressing is absL
JMPIMM       0   0100 1110 1111 1001   1 1     immedL    none
JMPIND       0   0100 1110 11SS Ssss   1 0     indAd16,indPCd16,indAd16,indAcp,indPCcp,indA    none

;JSRIMM modified to take argument directly from address translation
;original source addressing is absL
JSRIMM       0   0100 1110 1011 1001   1 1     immedL    none
JSRIND       0   0100 1110 10SS Ssss   1 0     indAd16,indPCd16,indAcp,indPCcp,indA    none

RTS          0   0100 1110 0111 0101   1 0     none    none
RTD          0   0100 1110 0111 0100   1 0     immedW    none
RTR          0   0100 1110 0111 0111   1 0     none    none
RTE          0   0100 1110 0111 0011   1 0     none    none

;---------------------------------------------------------------------
;*** Logic instructions
;*** Exclusive or
EORIMM2REGL  0   0000 1010 10DD Dddd   0 0     immedL    regD
EORIMM2REGW  0   0000 1010 01DD Dddd   0 0     immedW    regD
EORIMM2REGB  0   0000 1010 00DD Dddd   0 0     immedB    regD

EORIMM2MEML  0   0000 1010 10DD Dddd   0 0     immedL    indA,indApL,indmAL,indAd16,indAcp,absW,absL
EORIMM2MEMW  0   0000 1010 01DD Dddd   0 0     immedW    indA,indApW,indmAW,indAd16,indAcp,absW,absL
EORIMM2MEMB  0   0000 1010 00DD Dddd   0 0     immedB    indA,indApB,indmAB,indAd16,indAcp,absW,absL

EORREG2REGL  0   1011 sss1 10DD Dddd   0 0     regD    regD
EORREG2REGW  0   1011 sss1 01DD Dddd   0 0     regD    regD
EORREG2REGB  0   1011 sss1 00DD Dddd   0 0     regD    regD

EORREG2MEML  0   1011 sss1 10DD Dddd   0 0     regD    indA,indApL,indmAL,indAd16,indAcp,absW,absL
EORREG2MEMW  0   1011 sss1 01DD Dddd   0 0     regD    indA,indApW,indmAW,indAd16,indAcp,absW,absL
EORREG2MEMB  0   1011 sss1 00DD Dddd   0 0     regD    indA,indApB,indmAB,indAd16,indAcp,absW,absL

;*** And
ANDIMM2REGL  0   0000 0010 10DD Dddd   0 0     immedL    regD
ANDIMM2REGW  0   0000 0010 01DD Dddd   0 0     immedW    regD
ANDIMM2REGB  0   0000 0010 00DD Dddd   0 0     immedB    regD

ANDIMM2MEML  0   0000 0010 10DD Dddd   0 0     immedL    indA,indApL,indmAL,indAd16,indAcp,absW,absL
ANDIMM2MEMW  0   0000 0010 01DD Dddd   0 0     immedW    indA,indApW,indmAW,indAd16,indAcp,absW,absL
ANDIMM2MEMB  0   0000 0010 00DD Dddd   0 0     immedB    indA,indApB,indmAB,indAd16,indAcp,absW,absL

ANDREG2REGL  0   1100 ddd0 10SS Ssss   0 0     regD,regA    regD
ANDREG2REGW  0   1100 ddd0 01SS Ssss   0 0     regD,regA    regD
ANDREG2REGB  0   1100 ddd0 00SS Ssss   0 0     regD,regA    regD

ANDREG2MEML  0   1100 sss1 10DD Dddd   0 0     regD    indA,indApL,indmAL,indAd16,indAcp,absW,absL
ANDREG2MEMW  0   1100 sss1 01DD Dddd   0 0     regD    indA,indApW,indmAW,indAd16,indAcp,absW,absL
ANDREG2MEMB  0   1100 sss1 00DD Dddd   0 0     regD    indA,indApB,indmAB,indAd16,indAcp,absW,absL

ANDMEM2REGL  0   1100 ddd0 10SS Ssss   0 0     indA,indApL,indmAL,indAd16,indAcp,indPCcp,indPCd16,absW,absL    regD
ANDMEM2REGW  0   1100 ddd0 01SS Ssss   0 0     indA,indApW,indmAW,indAd16,indAcp,indPCcp,indPCd16,absW,absL    regD
ANDMEM2REGB  0   1100 ddd0 00SS Ssss   0 0     indA,indApB,indmAB,indAd16,indAcp,indPCcp,indPCd16,absW,absL    regD

